def add(x,y):
  result = x + y
  return(result)
  
adf = add(5,3)
print(adf)

def say_hello(name, surname):
    print(f'Hello, {name}, {surname}')

#named or keyword arguments
say_hello(surname="Bob", name ='Smith')

def divide(dividend,divisor):
    if divisor != 0:
        return dividend/divisor
    else:
        return 'you fool!'
        
result = divide(dividend=15,divisor=5)
print(result)
    

